{
    "name": "rssenal",
    "version": "0.5.0",
    "description": "RSS reader.",
    "main": "index.js",
    "scripts": {
        "test-client-singlerun": "jest --config=jest.client.config.json",
        "test-client-watch": "jest --watch --config=jest.client.config.json --no-watchman",
        "test-clearCache": "jest --clearCache",
        "build-client": "webpack --config webpack.config.js -d --watch",
        "clean-server": "rm -rf dist/server/*",
        "build-dev-server": "npm run clean-server && babel --watch src/server --out-dir dist/server --source-maps inline",
        "build-prod-server": "npm run clean-server && babel src/server --out-dir dist/server",
        "start-dev-server": "pm2 start pm2.config.json --env development",
        "start-prod-server": "npm run build-prod-server && pm2 start pm2.config.json --env production"
    },
    "keywords": [
        "rss",
        "react",
        "node",
        "mongodb"
    ],
    "author": "Destin Moulton",
    "license": "MIT",
    "dependencies": {
        "@types/debug": "^4.1.4",
        "@types/enzyme": "^3.1.10",
        "@types/html-entities": "^1.2.16",
        "@types/jest": "^24.0.15",
        "@types/lodash": "^4.14.111",
        "@types/react-dom": "^16.0.6",
        "@types/react-notification-system": "^0.2.37",
        "@types/react-redux": "^7.1.1",
        "@types/react-router-dom": "^4.2.7",
        "@types/react-router-redux": "^5.0.15",
        "@types/react-sortable-hoc": "^0.6.3",
        "@types/redux-logger": "^3.0.6",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "body-parser": "^1.18.3",
        "debug": "^4.0.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint-plugin-react": "^7.10.0",
        "express": "^4.16.3",
        "express-jwt": "^6.0.0",
        "feedparser-promised": "^2.0.1",
        "fetch-mock": "^7.3.6",
        "fetch-ponyfill": "^6.0.2",
        "immutable": "^3.8.2",
        "jest": "^24.8.0",
        "jest-enzyme": "^7.0.2",
        "jest-localstorage-mock": "^2.2.0",
        "lodash": "^4.17.10",
        "moment": "^2.22.2",
        "mongoose": "^5.2.3",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-notification-system": "^0.2.17",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "react-router-redux": "^4.0.8",
        "react-sortable-hoc": "^1.9.1",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0",
        "sanitize-html-react": "^1.13.0",
        "semantic-ui-css": "^2.3.3",
        "semantic-ui-react": "^0.87.3",
        "source-map-loader": "^0.2.4",
        "ts-jest": "^24.0.2",
        "ts-loader": "^6.0.4",
        "typescript": "^3.0.3",
        "webpack": "^4.19.0",
        "webpack-cli": "^3.1.0",
        "winston": "^3.1.0"
    }
}
